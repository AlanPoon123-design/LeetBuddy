{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "LeetBuddy",
    "action": {
      "default_icon": "lb_icon_head.png",
      "default_popup": "index.html"
    },
    "permissions": [
      "scripting",
      "activeTab",
      "storage",
      "cookies"
    ],
    "host_permissions": [
      "https://leetcode.com/problems/*",
      "http://leetcode.com/problems/*"
    ],
    "icons": {
      "16": "lb_icon_head.png",
      "32": "lb_icon_head.png",
      "128": "lb_icon_head.png"
    }
  }